          "options": {
            "alias": "temp_f",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "temperature_c"
                }
              },
              "operator": "*",
              "right": {
                "fixed": "1.8"
              }
            },
            "mode": "binary",
            "reduce": {
              "include": [
