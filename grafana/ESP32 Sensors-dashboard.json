          "format": "table",
          "hide": false,
          "range": true,
          "rawSql": "select\r\n  $__time(recorded_at),\r\n  (temperature_c * 9.0/5.0 + 32) as temperature_f\r\nfrom public.readings\r\nwhere device_id = 'esp32-lab-01'\r\norder by recorded_at\r\n",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${DS_GRAFANA-POSTGRESQL-DATASOURCE}"
          },
          "editorMode": "code",
          "format": "table",
          "hide": false,
          "range": true,
          "rawQuery": true,
          "rawSql": "select\r\n  date_trunc('day', recorded_at) + interval '4 hours' * floor(extract(hour from recorded_at) / 4) + interval '2 hours' as time,\r\n  avg(temperature_c * 9.0/5.0 + 32) as avg_temperature_f\r\nfrom public.readings\r\nwhere device_id = 'esp32-lab-01'\r\n  and $__timeFilter(recorded_at)\r\ngroup by date_trunc('day', recorded_at), floor(extract(hour from recorded_at) / 4)\r\norder by time\r\n",
          "refId": "B",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
